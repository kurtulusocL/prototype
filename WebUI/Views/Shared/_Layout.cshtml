<!DOCTYPE html>
<html lang="en">
<!-- schedule23:20-->
<head>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0">
	<title>Prototype | @ViewData["Title"]</title>
	<link rel="stylesheet" type="text/css" href="~/temp/assets/css/bootstrap.min.css">
	<link rel="stylesheet" type="text/css" href="~/temp/assets/css/font-awesome.min.css">
	<link rel="stylesheet" type="text/css" href="~/temp/assets/css/select2.min.css">
	<link rel="stylesheet" type="text/css" href="~/temp/assets/css/bootstrap-datetimepicker.min.css">
	<link rel="stylesheet" type="text/css" href="~/temp/assets/css/style.css">
	<!--[if lt IE 9]>
	<script src="assets/js/html5shiv.min.js"></script>
	<script src="assets/js/respond.min.js"></script>
	<![endif]-->
</head>

<body>
	<div class="main-wrapper">
		<div class="header"></div>
		@if (User.Identity.IsAuthenticated == true)
		{
			<div class="sidebar" id="sidebar">
				<div class="sidebar-inner slimscroll">
					<div id="sidebar-menu" class="sidebar-menu">
						<ul>
							<li class="menu-title">Dashboard</li>
							<li>
								<a asp-controller="Audit" asp-action="Index"> <span>Audit</span></a>
							</li>
							<li>
								<a asp-controller="Category" asp-action="Index"> <span>Category</span></a>
							</li>
							<li>
								<a asp-controller="Subcategory" asp-action="Index"> <span>Subcategory</span></a>
							</li>
							<li>
								<a asp-controller="Stock" asp-action="Index"> <span>Stock</span></a>
							</li>
							<li>
								<a asp-controller="Product" asp-action="Index"> <span>Product</span></a>
							</li>
							<li>
								<a asp-controller="Picture" asp-action="Index"> <span>Images</span></a>
							</li>
							<li>
								<a asp-controller="User" asp-action="Index"> <span>User</span></a>
							</li>
							<li>
								<a asp-controller="UserRole" asp-action="Index"> <span>User Role</span></a>
							</li>
							<li>
								<a asp-controller="ExceptionLogger" asp-action="Index"> <span>Exception Logs</span></a>
							</li>
							<li>
								<a asp-controller="Account" asp-action="Logout"> <span>Logout</span></a>
							</li>							
						</ul>
					</div>
				</div>
			</div>
			
		}
		@RenderBody()
	</div>
	<div class="sidebar-overlay" data-reff=""></div>
	<script src="~/temp/assets/js/jquery-3.2.1.min.js"></script>
	<script src="~/js/select.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
	<script src="~/js/deletealert.js"></script>
	<script src="~/temp/assets/js/popper.min.js"></script>
	<script src="~/temp/assets/js/bootstrap.min.js"></script>
	<script src="~/temp/assets/js/jquery.slimscroll.js"></script>
	<script src="~/temp/assets/js/select2.min.js"></script>
	<script src="~/temp/assets/js/moment.min.js"></script>
	<script src="~/temp/assets/js/bootstrap-datetimepicker.min.js"></script>
	<script src="~/temp/assets/js/app.js"></script>
</body>
<!-- schedule23:21-->
</html>